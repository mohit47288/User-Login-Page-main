<!DOCTYPE html>
<html>
<head>
  <title>User Table</title>
  <link rel="stylesheet" href="UserTable.css" />
</head>
<body>
  <h1>User Table</h1>
  <table border="1">
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Mobile</th>
      <th>Email</th>
      <th>Street</th>
      <th>City</th>
      <th>State</th>
      <th>Country</th>
      <th>Username</th>
      <!-- <th>Password (Hashed)</th> -->
    </tr>
    <% users.forEach(user => { %>
      <tr>
        <td><%= user.firstName %></td>
        <td><%= user.lastName %></td>
        <td><%= user.mobile %></td>
        <td><%= user.email %></td>
        <td><%= user.street %></td>
        <td><%= user.city %></td>
        <td><%= user.state %></td>
        <td><%= user.country %></td>
        <td><%= user.username %></td>
        <!-- <td><%= user.password %></td> -->
      </tr>
    <% }) %>
  </table>
  <a href="/">Go Back to Login</a>
</body>
</html>
